#include <bits/stdc++.h> 
using namespace std;
# define ll long long
#define mod 1000000007
int main() 
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin >> t;
    while (t--)
    {
        int n, rev=0, digit;
        cin >> n;
        while(n != 0)
        {
            rev= (rev*10) + n%10;
            n /= 10;
        }
        cout << rev << '\n';
    }
    return 0;
}
