
#include <bits/stdc++.h> 
using namespace std;
# define ll long long
#define mod 1000000007

ll Pow(ll a,ll x) { ll res=1,tmp=a; while(x){ if(x&1) res=res*tmp%mod; tmp=tmp*tmp%mod; x>>=1; } return res; } // Exponennt

ll Inv(ll x) { return Pow(x, mod-2); } // Inverse of X;

ll fact(ll n) { ll fact = 1; for (ll i = 2; i <= n; i++) fact *= i; return fact; } //factorial 

ll ncr(ll n, ll r) { ll ncr = fact(n) / (fact(r) * fact(n - r)); return ncr; } //combination 

int main() 
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    return 0;
}
